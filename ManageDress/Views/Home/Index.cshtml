    <script>
        function closeVideo() {
            setTimeout(function () {
                if ($("#video-banner").is(":hidden")) {
                    if ($("#video-banner-mobile").parent().hasClass('active')) {
                        /*$('#video-banner-mobile').trigger('play');*/
                    } else {
                        $('#video-banner-mobile').trigger('pause');
                    }
                }
                else {
                    if ($("#video-banner").parent().hasClass('active')) {
                          /*$('#video-banner').trigger('play');*/
                    } else {
                        $('#video-banner').trigger('pause');
                    }
                }
            }, 1000);
        }

        function viewAll(index) {
            if (index == 1) {
                var text = '@Model.ProcessSurgerys.Find(m => m.Index == 1).Content';
                $(".step1").css("height", "190px");
                $(".step1").html(text);
            }
            if (index == 2) {
                $(".step2").html('@Model.ProcessSurgerys.Find(m => m.Index == 2).Content');
            }
            if (index == 3) {
                var text = `@Model.ProcessSurgerys.Find(m => m.Index == 3).Content`;
                $(".step3_width").css("height", "190px");
                $(".step3").html(text);
            }
            if (index == 4) {
                $(".step4_width").css("height", "225px");
                $(".step4").html(`@Model.ProcessSurgerys.Find(m => m.Index == 4).Content`);
            }
            if (index == 5) {
                $(".step5").html(`@Model.ProcessSurgerys.Find(m => m.Index == 5).Content`);
            }
            if (index == 6) {
                $(".step6_width").css("height", "345px");
                $(".step6").html(`@Model.ProcessSurgerys.Find(m => m.Index == 6).Content`);
            }
        }

        $(document).ready(function () {

            if ($("#video-banner").is(":hidden")) {
                $('#video-banner').trigger('pause');
            }

            if ($("#video-banner-mobile").is(":hidden")) {
                $('#video-banner-mobile').trigger('pause');
            }

            $("#customerImageMobile").on('slide.bs.carousel', function (e) {
                $('.mobile-modal').modal('hide');
            });

            $("#main-banner").on('slide.bs.carousel', function (e) {

                setTimeout(function () {

                    if ($("#video-banner").is(":hidden")) {
                        $('#video-banner').trigger('pause');

                        if ($("#video-banner-mobile").parent().hasClass('active')) {
                            /*$('#video-banner-mobile').trigger('play');*/
                        } else {
                            $('#video-banner-mobile').trigger('pause');
                        }
                    }
                    else {
                        $('#video-banner-mobile').trigger('pause');
                        if ($("#video-banner").parent().hasClass('active')) {
                            /*$('#video-banner').trigger('play');*/
                        } else {
                            $('#video-banner').trigger('pause');
                        }
                    }
                }, 1000);
            });

            $('.txtSeach').on('keyup', function (e) {
                if (e.keyCode == 13) {
                    var txtText = $('.txtSeach').val();
                    if (txtText.length == 0) {
                        swal("", "Bạn cần nhập text để tìm kiếm");
                    } else {
                        location.href = '/Home/Search?Key=' + txtText;
                    }
                }
            });

            $(".search_box").change(function (e) {
                var txtText = $('.txtSeach').val();
                if (txtText.length == 0) {
                    swal("", "Bạn cần nhập text để tìm kiếm");
                } else {
                    location.href = '/Home/Search?Key=' + txtText;
                }
            });

            $("#search").click(function () {
                location.href = '/Home/Search?Key=' + $("#search-value").val();
            });

            $('#search-value').keypress(function (e) {
                var key = e.which;
                if (key == 13)  // the enter key code
                {
                    $('#search').click();
                    return false;
                }
            });

            function showStartPage() {
                $("#popup").hide().fadeIn(500);
                $("html").scrollTop(0);
            }

            $(".close-div").click(function () {
                $("#popup").fadeOut(500);
                $("html").css("overflow-y", "auto");
            });

            $('[data-toggle="tooltip"]').tooltip();

            $("#popup").hide();
            setTimeout(showStartPage, 1000);
            //close the POPUP if the button with id="close" is clicked
            $("#popup").on("click", function (e) {
                e.preventDefault();
                $("#popup").fadeOut(500);
                $("html").css("overflow-y", "auto");
                /*document.getElementById("tuvan").scrollIntoView();*/
            });

            $("#btnImage").click(function () {
                $('#register-model').modal('show');
            });

            $('.img-product').hover(function () {
                if ($(this).children().hasClass("show")) {
                    $(this).children("h2").removeClass("show");
                    $(this).children("h2").addClass("hide");
                    $(this).children("h2").hide();
                    $(this).children("img").css("border", "none");
                }
                else {
                    $(this).children("h2").removeClass("hide");
                    $(this).children("h2").addClass("show");
                    $(this).children("h2").show();
                    $(this).children("img").css("border", "4px solid #844400");
                }
            });

            function hex_initial_animation() {
                $(".hex-wrap,.hover-notify").velocity("transition.expandIn", { stagger: 150 });
                $(".hex-wrap").velocity("callout.pulse");
                $(".hoverblock").velocity("fadeOut", { delay: 3000, duration: 0 });
            }

            hex_initial_animation();

            setTimeout(function () {
                $('.@Model.ProcessSurgerys.Find(m => m.Index == 2).Key').addClass('desc-active');
                $('.code-descriptopn > div, .desc-active').children().velocity('stop', true).velocity("transition.slideRightBigIn", { stagger: 300 });

            }, 0);

            var hoverdetect = setInterval(function () { hovernotify() }, 3000);
            function hovernotify() {
                $(".hover-notify").velocity("callout.tada");
            }

            function myStopFunction() {
                $(".hover-notify").velocity('stop', true).velocity("fadeOut");
                clearInterval(hoverdetect);
            }

            $(".hex-init").mouseenter(function () {

                myStopFunction();

                var title_color = $(this).parent().attr("data-color");
                var title_name = $(this).parent().attr("data-title");
                var desc_name = $(this).parent().attr("data-content");

                function hex_description() {
                    $('.code-description').velocity('stop', true).velocity("transition.slideRightBigIn");
                    $('.' + desc_name).siblings().removeClass('desc-active');
                    setTimeout(function () {
                        $('.' + desc_name).addClass('desc-active');
                        $('.code-descriptopn > div, .desc-active').children().velocity('stop', true).velocity("transition.slideRightBigIn", { stagger: 300 });
                        $('.code-title, .desc-active span').velocity({ color: title_color }, { queue: false });
                        $('.code-title').text(title_name)
                    }, 0);
                }
                hex_description();

                $(this).parent().addClass('hexactive');
                $('.hexactive').velocity({ scaleX: "1.1", scaleY: "1.1" }, { duration: 200 });

            }).mouseleave(function () {
                $('.hexactive').velocity('stop', true).velocity('reverse').removeClass('hexactive');
            });

            $('.videoxy').YouTubePopUp();

            $('#btnSendContact').click(function () {
				if ($('#txtEmail').val() != "" && $('#txtName').val() != "" && $('#txtPhone').val() != "") {
                	AddFeedback('txtEmail', 'txtName', 'txtPhone', 'txtNote');
				}
            });

            $('#btnSendContact1').click(function () {
				if ($('#txtEmail_1').val() != "" && $('#txtName_1').val() != "" && $('#txtPhone_1').val() != "") {
                	AddFeedback('txtEmail_1', 'txtName_1', 'txtPhone_1', 'txtNote_1');
				}
            });

            $('#submitFeedback').click(function () {
                if ($('#txtEmailAddress').val() != "" && $('#txtFullName').val() != "" && $('#txtPhoneNumber').val() != "") {
                    AddFeedback('txtEmailAddress', 'txtFullName', 'txtPhoneNumber', 'txtContent');
                }
                else {
                    alert("Bạn cần nhập đầy đủ thông tin yêu cầu tư vấn !!");
                }
            });

            function AddFeedback(email, name, phone, note) {
                var model = {
                    'Email': $('#' + email).val(),
                    'Name': $('#' + name).val(),
                    'Phone': $('#' + phone).val(),
                    'Note': $('#' + note).val()
                };

                $.ajax({
                    type: "POST",
                    url: "@Url.Action("AddFeedback")",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify(model),
                    success: function (data) {
                        if (data.Response == "Success") {
                            $('#' + email).val("");
                            $('#' + name).val("");
                            $('#' + phone).val("");
                            $('#' + note).val("");
                            $("html").scrollTop(0);
                            alert("Gửi yêu cầu thành công.");
                            $('#register-model').modal('hide');
                        }
                    },
                    failure: function (errMsg) {
                        alert(errMsg);
                    }
                });
            }
        });
    </script>
