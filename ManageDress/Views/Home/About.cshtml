@model IEnumerable<Dress_BLL.Model.UserModel>
@{
    ViewData["Title"] = "Administration Console";
    ViewBag.Home = "active";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h2>List Users</h2>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>Phone Number</th>
                <th>Gender</th>
                <th>Date of birth</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.FullName</td>
                    <td>@item.Phone</td>
                    <td>@item.Gender</td>
                    <td>@item.DateofBirth</td>
                </tr>
            }
        </tbody>
    </table>
</div>